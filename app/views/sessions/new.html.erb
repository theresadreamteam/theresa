<body class="newWalk">
<div id="logInBlock">
<h1>Login</h1>
   <%= form_tag '/login' do %>
   <%= label_tag :username%>
   <%= text_field_tag :username %>
   <br>
   <%= label_tag :password%>
   <%= password_field_tag :password %>
   <%= submit_tag "Login" %>
   <a type="button" class="btn btn-default" id= "signup" href="<%= new_user_path %>">Sign up</a>
<%end%>
</div>
</body>